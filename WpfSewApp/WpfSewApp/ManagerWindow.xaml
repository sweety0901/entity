<Window x:Class="WpfSewApp.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSewApp"
        mc:Ignorable="d"
        Title="ManagerWindow" Height="300" Width="300" Background="#FFB5D5CA" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="BAllOrder" Content="Все заказы" HorizontalAlignment="Left" Padding="5" Margin="94,63,0,0" VerticalAlignment="Top" Width="121" Click="BAllOrder_Click" Background="#FFE0A9AF"/>
        <Button x:Name="BMyOrder" Content="Текущие заказы" HorizontalAlignment="Left" Padding="5" Margin="94,105,0,0" VerticalAlignment="Top" Width="121" Click="BMyOrder_Click" Background="#FFE0A9AF"/>

    </Grid>
</Window>
